block pageVars
	- var pageTitle    = "Keytable Datatable"
	- var pageSubTitle = "Excel like cell navigation in DataTables"
	- var description  = "KeyTable provides Excel like cell navigation on any table. Events (focus, blur, action etc) can be assigned to individual cells, columns, rows or all cells."
	- var activeMenu   = "dt-extensions-key-table"

extends template

//- Page specific dependency
//------------------------------

//- Add custom page specific CSS
append breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"DataTables"}, {name:"Keytable Datatable"}])



//- Include page content in page block
append content
	include ../contents/dt-extensions-key-table.html

//- Vendor CSS
//------------------------------
//- Add Vendor specific CSS
append vendorcss
	link(rel='stylesheet', type='text/css', href='../../../app-assets/vendors/css/tables/datatable/dataTables.bootstrap4.min.css')
	link(rel='stylesheet', type='text/css', href='../../../app-assets/vendors/css/tables/datatable/select.dataTables.min.css')
	link(rel='stylesheet', type='text/css', href='../../../app-assets/vendors/css/tables/extensions/keyTable.dataTables.min.css')

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src='../../../app-assets/vendors/js/tables/jquery.dataTables.min.js', type='text/javascript')
	script(src='../../../app-assets/vendors/js/tables/datatable/dataTables.bootstrap4.min.js', type='text/javascript')
	script(src='../../../app-assets/vendors/js/tables/datatable/dataTables.fixedHeader.min.js', type='text/javascript')
	script(src='../../../app-assets/vendors/js/tables/datatable/dataTables.keyTable.min.js',type='text/javascript')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	//- Custom JS
	script(src='../../../app-assets/js/scripts/tables/datatables-extensions/datatable-keytable.js',type='text/javascript')